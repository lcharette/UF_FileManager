{
    "name": "lcharette/uf-files",
    "type": "userfrosting-sprinkle",
    "description": "Filemanager Sprinkle for UserFrosting.",
    "homepage": "https://github.com/archey347/uf-filemanager",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Archey Barrell",
            "homepage": "https://github.com/archey347"
        }
    ],
    "require": {
        "php": ">=7.0",
        "league/flysystem": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\Files\\": "src/"
        },
        "files" : [
            "defines.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\Files\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "Files"
    }
}